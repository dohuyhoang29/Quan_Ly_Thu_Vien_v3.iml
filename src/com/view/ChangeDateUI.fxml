<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="225.0" prefWidth="394.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.controller.ChangeDateController">
   <children>
      <Label layoutX="67.0" layoutY="23.0" text="%lb.choose">
         <font>
            <Font name="System Bold" size="24.0" />
         </font></Label>
      <DatePicker fx:id="dtpChangeDate" layoutX="110.0" layoutY="88.0" />
      <Button fx:id="btnCancel" layoutX="67.0" layoutY="151.0" mnemonicParsing="false" onAction="#clickCancel" prefHeight="39.0" prefWidth="106.0" text="%btn.cancel">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnSubmit" layoutX="221.0" layoutY="151.0" mnemonicParsing="false" onAction="#clickSubmit" prefHeight="39.0" prefWidth="106.0" text="%btn.submit">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
